{
	"name": "@sapphire/cf-workers-slash-commands-docs",
	"version": "1.0.0",
	"description": "Cloudflare Worker Slash Commands handler for documentation purposes",
	"main": "dist/index.js",
	"license": "MIT",
	"private": true,
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write src/**/*.ts",
		"update": "yarn upgrade-interactive --latest",
		"build": "rollup -c",
		"watch": "rollup -wc",
		"commit": "git-cz",
		"cz": "git-cz",
		"prepare": "husky install .github/husky"
	},
	"dependencies": {
		"discord-api-types": "^0.18.1",
		"tslib": "^2.2.0",
		"tweetnacl": "^1.0.3"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^2.2.2",
		"@cloudflare/wrangler": "^1.16.1",
		"@commitlint/cli": "^12.1.1",
		"@commitlint/config-conventional": "^12.1.1",
		"@rollup/plugin-commonjs": "^19.0.0",
		"@rollup/plugin-node-resolve": "^13.0.0",
		"@sapphire/eslint-config": "^3.1.2",
		"@sapphire/prettier-config": "^1.1.1",
		"@sapphire/ts-config": "^2.2.1",
		"cz-conventional-changelog": "^3.3.0",
		"dotenv": "^9.0.2",
		"husky": "^6.0.0",
		"lint-staged": "^11.0.0",
		"prettier": "^2.3.0",
		"pretty-quick": "^3.1.0",
		"rollup": "^2.47.0",
		"rollup-plugin-cleaner": "^1.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.30.0",
		"rollup-plugin-inject-process-env": "^1.3.1",
		"typescript": "^4.2.4"
	},
	"files": [],
	"engines": {
		"node": ">=15",
		"npm": ">=6"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"resolutions": {
		"acorn": "^8.2.4",
		"minimist": "^1.2.5",
		"kind-of": "^6.0.3",
		"dot-prop": "^6.0.1",
		"marked": "^2.0.3",
		"merge": "^2.1.1",
		"trim": "^1.0.1"
	},
	"prettier": "@sapphire/prettier-config"
}
