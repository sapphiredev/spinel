{
	"name": "spinel",
	"version": "2.0.0",
	"description": "Slash Commands handler Sapphire Discord server",
	"author": "@sapphire <contact@sapphirejs.dev>",
	"license": "MIT",
	"private": true,
	"main": "dist/main.mjs",
	"type": "module",
	"imports": {
		"#lib/*": "./dist/lib/*.mjs",
		"#utils/*": "./dist/lib/util/*.mjs",
		"#types/*": "./dist/lib/types/*.mjs",
		"#constants/*": "./dist/lib/constants/*.mjs"
	},
	"scripts": {
		"lint": "eslint src tests --ext ts --fix",
		"format": "prettier --write src tests",
		"test": "vitest run",
		"typecheck": "tsc -b src",
		"start": "node --preserve-symlinks --enable-source-maps dist/main.mjs",
		"build": "tsdown",
		"watch": "tsdown --watch",
		"dev": "tsdown --watch --onSuccess \"yarn start\"",
		"clean": "rimraf dist/",
		"validate-tags": "tsx scripts/validate-tags.ts"
	},
	"dependencies": {
		"@discordjs/builders": "^1.13.0",
		"@discordjs/collection": "^2.1.1",
		"@sapphire/fetch": "^3.0.5",
		"@sapphire/result": "^2.8.0",
		"@sapphire/utilities": "^3.18.2",
		"@skyra/env-utilities": "^2.0.1",
		"@skyra/http-framework": "^2.2.1",
		"@skyra/jaro-winkler": "^1.1.1",
		"he": "^1.2.0",
		"ioredis": "^5.8.2",
		"smol-toml": "^1.5.2",
		"turndown": "^7.2.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@sapphire/eslint-config": "^5.0.6",
		"@sapphire/prettier-config": "^2.0.0",
		"@sapphire/ts-config": "^5.0.3",
		"@types/he": "^1.2.3",
		"@types/node": "^24.10.1",
		"@types/turndown": "^5.0.6",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"@vitest/coverage-v8": "^4.0.14",
		"colorette": "^2.0.20",
		"cz-conventional-changelog": "^3.3.0",
		"esbuild": "^0.27.0",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"lint-staged": "^16.2.7",
		"prettier": "^3.7.1",
		"rimraf": "^6.1.2",
		"tsdown": "^0.16.8",
		"tsx": "^4.20.6",
		"typescript": "~5.9.3",
		"vite": "^7.2.4",
		"vitest": "^4.0.14"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write",
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"prettier": "@sapphire/prettier-config",
	"eslintConfig": {
		"extends": "@sapphire"
	},
	"packageManager": "yarn@4.12.0",
	"volta": {
		"node": "24.11.1"
	}
}
