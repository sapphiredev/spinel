{
	"name": "@sapphire/slashies-handler",
	"version": "1.0.0",
	"description": "Slash Commands handler Sapphire Discord server",
	"author": "@sapphire <contact@sapphirejs.dev>",
	"license": "MIT",
	"private": true,
	"main": "dist/main.js",
	"type": "module",
	"imports": {
		"#lib/*": "./dist/lib/*.js",
		"#select-menus/*": "./dist/select-menus/*.js",
		"#commands/*": "./dist/commands/*.js",
		"#utils/*": "./dist/lib/util/*.js",
		"#env/*": "./dist/lib/env/*.js",
		"#types/*": "./dist/lib/types/*.js",
		"#constants/*": "./dist/lib/constants/*.js",
		"#api/*": "./dist/lib/api/*.js",
		"#root/*": "./dist/*.js"
	},
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write src",
		"test": "jest",
		"typecheck": "tsc -b src",
		"dev": "yarn build && yarn start",
		"start": "node dist/main.js",
		"build": "node scripts/build.mjs",
		"watch": "node scripts/watch.mjs",
		"clean": "node scripts/clean.mjs",
		"update-slashies": "node ./scripts/interactions/updateSlashCommands.mjs",
		"update-guild-slashies": "node ./scripts/interactions/updateGuildSlashCommands.mjs",
		"update": "yarn upgrade-interactive",
		"validate-tags": "node scripts/validateTags.mjs",
		"commit": "git-cz",
		"cz": "git-cz",
		"prepare": "husky install .github/husky"
	},
	"dependencies": {
		"@discordjs/builders": "^0.7.0",
		"@discordjs/collection": "^0.3.2",
		"@ltd/j-toml": "^1.23.0",
		"@sapphire/fetch": "^2.0.2",
		"@sapphire/time-utilities": "^1.4.6",
		"@sapphire/utilities": "^3.0.6",
		"@skyra/jaro-winkler": "^1.0.0",
		"colorette": "^2.0.16",
		"discord-api-types": "^0.24.0",
		"discord.js-docs": "teeseal/discord.js-docs",
		"dotenv-cra": "^3.0.2",
		"fastify": "^3.22.1",
		"he": "^1.2.0",
		"tslib": "^2.3.1",
		"turndown": "^7.1.1",
		"tweetnacl": "^1.0.3"
	},
	"devDependencies": {
		"@commitlint/cli": "^13.2.1",
		"@commitlint/config-conventional": "^13.2.0",
		"@sapphire/eslint-config": "^4.0.2",
		"@sapphire/prettier-config": "^1.2.3",
		"@sapphire/ts-config": "^3.1.3",
		"@types/he": "^1.1.2",
		"@types/jest": "^27.0.2",
		"@types/node": "^16.11.6",
		"@types/turndown": "^5.0.1",
		"@typescript-eslint/eslint-plugin": "^5.2.0",
		"@typescript-eslint/parser": "^5.2.0",
		"cz-conventional-changelog": "^3.3.0",
		"esbuild": "^0.13.11",
		"esbuild-jest": "^0.5.0",
		"eslint": "^8.1.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.4",
		"jest": "^27.3.1",
		"jest-circus": "^27.3.1",
		"lint-staged": "^11.2.6",
		"prettier": "^2.4.1",
		"pretty-quick": "^3.1.1",
		"typescript": "^4.4.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphiredev/sapphire-slashies.git"
	},
	"bugs": {
		"url": "https://github.com/sapphiredev/sapphire-slashies/issues"
	},
	"homepage": "https://www.sapphirejs.dev/",
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"resolutions": {
		"ansi-regex": "^5.0.1"
	},
	"prettier": "@sapphire/prettier-config",
	"eslintConfig": {
		"extends": "@sapphire"
	},
	"packageManager": "yarn@3.0.2"
}
