{
	"name": "@sapphire/slashies-handler",
	"version": "2.0.0",
	"description": "Slash Commands handler Sapphire Discord server",
	"author": "@sapphire <contact@sapphirejs.dev>",
	"license": "MIT",
	"private": true,
	"main": "dist/main.js",
	"type": "module",
	"imports": {
		"#lib/*": "./dist/lib/*.js",
		"#utils/*": "./dist/lib/util/*.js",
		"#types/*": "./dist/lib/types/*.js",
		"#constants/*": "./dist/lib/constants/*.js"
	},
	"scripts": {
		"lint": "eslint src tests --ext ts --fix",
		"format": "prettier --write src tests",
		"test": "jest",
		"typecheck": "tsc -b src",
		"start": "node --preserve-symlinks --enable-source-maps dist/main.js",
		"build": "tsup",
		"watch": "tsup --watch",
		"dev": "tsup --watch --onSuccess \"yarn start\"",
		"clean": "node scripts/clean.mjs",
		"validate-tags": "node scripts/validateTags.mjs",
		"postinstall": "husky install .github/husky"
	},
	"dependencies": {
		"@discordjs/builders": "0.16.0-dev.1655640244-358c3f4",
		"@discordjs/collection": "0.8.0-dev.1655640259-358c3f4",
		"@ltd/j-toml": "^1.30.0",
		"@sapphire/discord-utilities": "^2.11.2",
		"@sapphire/fetch": "^2.4.0",
		"@sapphire/result": "^1.1.1",
		"@sapphire/time-utilities": "^1.7.4",
		"@sapphire/utilities": "^3.6.2",
		"@skyra/env-utilities": "^1.0.1",
		"@skyra/http-framework": "^0.5.5",
		"@skyra/jaro-winkler": "^1.1.0",
		"discordjs-docs-parser": "^1.2.0",
		"he": "^1.2.0",
		"ioredis": "^5.0.6",
		"tslib": "^2.4.0",
		"turndown": "^7.1.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.2",
		"@commitlint/config-conventional": "^17.0.2",
		"@sapphire/eslint-config": "^4.3.7",
		"@sapphire/prettier-config": "^1.4.3",
		"@sapphire/ts-config": "^3.3.4",
		"@types/he": "^1.1.2",
		"@types/jest": "^28.1.2",
		"@types/node": "^18.0.0",
		"@types/turndown": "^5.0.1",
		"@typescript-eslint/eslint-plugin": "^5.28.0",
		"@typescript-eslint/parser": "^5.28.0",
		"colorette": "^2.0.19",
		"cz-conventional-changelog": "^3.3.0",
		"esbuild": "^0.14.46",
		"esbuild-jest": "^0.5.0",
		"eslint": "^8.18.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^8.0.1",
		"jest": "^28.1.1",
		"lint-staged": "^13.0.2",
		"prettier": "^2.7.1",
		"pretty-quick": "^3.1.3",
		"tsup": "^6.1.2",
		"typescript": "^4.7.4"
	},
	"resolutions": {
		"ansi-regex": "^5.0.1",
		"minimist": "^1.2.6"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"prettier": "@sapphire/prettier-config",
	"eslintConfig": {
		"extends": "@sapphire"
	},
	"packageManager": "yarn@3.2.1"
}
